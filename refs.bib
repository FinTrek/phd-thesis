@Article{10.1371/journal.pgen.1001319,
  author    = {Mailund, Thomas AND Dutheil, Julien Y. AND Hobolth, Asger AND Lunter, Gerton AND Schierup, Mikkel H.},
  title     = {Estimating Divergence Time and Ancestral Effective Population Size of Bornean and Sumatran Orangutan Subspecies Using a Coalescent Hidden Markov Model},
  journal   = {PLoS Genet},
  year      = {2011},
  volume    = {7},
  number    = {3},
  pages     = {e1001319},
  month     = {03},
  abstract  = {<title>Author Summary</title>
<p>We present a hidden Markov model that uses variation in coalescence times between two distantly related populations, or closely related species, to infer population genetics parameters in ancestral population or species. The model infers the divergence times in segments along the alignment. Using coalescent simulations, we show that the model accurately estimates the divergence time between the two populations and the effective population size of the ancestral population. We apply the model to the recently sequenced orangutan sub-species and estimate their divergence time and the effective population size of their ancestor population.</p>
},
  doi       = {10.1371/journal.pgen.1001319},
  publisher = {Public Library of Science},
  url       = {http://dx.doi.org/10.1371%2Fjournal.pgen.1001319},
}

@Article{10.1371/journal.pgen.0030007,
  author    = {Hobolth, Asger AND Christensen, Ole F AND Mailund, Thomas AND Schierup, Mikkel H},
  title     = {Genomic Relationships and Speciation Times of Human, Chimpanzee, and Gorilla Inferred from a Coalescent Hidden Markov Model},
  journal   = {PLoS Genet},
  year      = {2007},
  volume    = {3},
  number    = {2},
  pages     = {e7},
  month     = {02},
  abstract  = {<title>Author Summary</title><sec id="st1"><title/><p>Primate evolution is a central topic in biology and much information can be obtained from DNA sequence data. A key parameter is the time ‚Äúwhen we became human,‚Äù i.e., the time in the past when descendents of the human‚Äìchimp ancestor split into human and chimpanzee. Other important parameters are the time in the past when descendents of the human‚Äìchimp‚Äìgorilla ancestor split into descendents of the human‚Äìchimp ancestor and the gorilla ancestor, and population sizes of the human‚Äìchimp and human‚Äìchimp‚Äìgorilla ancestors. To estimate speciation times and ancestral population sizes we have developed a new methodology that explicitly utilizes the spatial information in contiguous genome alignments. Furthermore, we have applied this methodology to four long autosomal human‚Äìchimp‚Äìgorilla‚Äìorangutan alignments and estimated a very recent speciation time of human and chimp (around 4 million years) and ancestral population sizes much larger than the present-day human effective population size. We also analyzed X-chromosome sequence data and found that the X chromosome has experienced a different history from that of autosomes, possibly because of selection.</p></sec>},
  doi       = {10.1371/journal.pgen.0030007},
  publisher = {Public Library of Science},
  url       = {http://dx.plos.org/10.1371%2Fjournal.pgen.0030007},
}

